Cell[TextData[{
 "Note that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"O", "(", 
      SuperscriptBox["h", "2"], ")"}], "-", 
     RowBox[{"O", "(", 
