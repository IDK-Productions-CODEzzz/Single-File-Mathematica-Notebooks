Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "Clear", " ", "all", " ", "variables", " ", "to", " ", "ensure", " ", 
    "code", " ", "below", " ", "works"}], " ", "*)"}], "\[IndentingNewLine]", 
  
  RowBox[{
   RowBox[{
    RowBox[{"ClearAll", "[", "\"\<Global`*\>\"", "]"}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Code", " ", "that", " ", "constructs", " ", "the", " ", "visual", " ", 
     "of", " ", "the", " ", "scheme", " ", "approximating", " ", "the", " ", 
     "true", " ", "solution"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"scheme", " ", ":=", " ", 
     RowBox[{"DynamicModule", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "f", ",", " ", "x", ",", " ", "a", ",", " ", "b", ",", " ", "u0", ",",
          " ", "h", ",", " ", "xGrid", ",", " ", "i", ",", " ", "n", ",", " ",
          "A", ",", " ", "fVec", ",", " ", "uVec", ",", " ", "uMat", ",", " ",
          "sol", ",", " ", "u", ",", " ", "solPlot", ",", " ", "approxPlot"}],
         "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{
         RowBox[{"Step", " ", "1"}], ":", " ", 
         RowBox[{"Define", " ", "given", " ", "variables", " ", 
          RowBox[{"(", 
           RowBox[{"feel", " ", "free", " ", "to", " ", "edit"}], ")"}]}]}], 
        " ", "*)"}], "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"f", "[", "x_", "]"}], " ", "=", " ", 
         RowBox[{"Cos", "[", "x", "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"a", ",", "b"}], "}"}], " ", "=", " ", 
         RowBox[{"{", 
          RowBox[{"0.", ",", " ", 
           RowBox[{"2.", "*", "\[Pi]"}]}], "}"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"u0", " ", "=", " ", "0."}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
          RowBox[{"Step", " ", "2"}], ":", " ", 
          RowBox[{"Initialize", " ", 
           StyleBox["h",
            FontWeight->"Bold"]}]}], " ", "*)"}], "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"h", "[", "n_", "]"}], " ", ":=", " ", 
         FractionBox[
          RowBox[{"b", "-", "a"}], "n"]}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
          RowBox[{"Step", " ", "3"}], ":", " ", 
          RowBox[{"Construct", " ", "the", " ", "mesh"}]}], " ", "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"xGrid", "[", "n_", "]"}], " ", ":=", " ", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"a", "+", 
            RowBox[{"i", "*", 
             RowBox[{"h", "[", "n", "]"}]}]}], ",", " ", 
           RowBox[{"{", 
            RowBox[{"i", ",", " ", "0", ",", " ", "n"}], "}"}]}], "]"}]}], 
        ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
          RowBox[{"Steps", " ", "4"}], ",", " ", "5", ",", " ", 
          RowBox[{"8", ":", " ", 
           RowBox[{"Initialize", " ", "sparse", " ", "array", " ", 
            StyleBox["A",
             FontWeight->"Bold"]}]}]}], " ", "*)"}], "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"A", "[", "n_", "]"}], " ", ":=", " ", 
         RowBox[{"SparseArray", "[", 
          RowBox[{"{", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{
             RowBox[{"{", 
              RowBox[{"1", ",", "1"}], "}"}], "->", "1."}], ",", " ", 
            RowBox[{"(*", " ", 
             RowBox[{"First", " ", "row"}], " ", "*)"}], 
            "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"n", "+", "1"}], ",", "n"}], "}"}], "->", 
             RowBox[{"-", "0.5"}]}], ",", " ", 
            RowBox[{"(*", " ", 
             RowBox[{
              RowBox[{"Last", " ", "row"}], ",", " ", 
              RowBox[{"2", "nd", " ", "to", " ", "last", " ", "column"}]}], 
             " ", "*)"}], "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"n", "+", "1"}], ",", 
               RowBox[{"n", "+", "1"}]}], "}"}], "->", "0.5"}], ",", " ", 
            RowBox[{"(*", " ", 
             RowBox[{
              RowBox[{"Last", " ", "row"}], ",", " ", 
              RowBox[{"last", " ", "column"}]}], " ", "*)"}], 
            "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{
              RowBox[{"{", 
               RowBox[{"i_", ",", "j_"}], "}"}], "/;", 
              RowBox[{"(", 
               RowBox[{
                RowBox[{
                 RowBox[{"i", "-", "j"}], "==", "1"}], "&&", 
                RowBox[{"i", "<", 
                 RowBox[{"n", "+", "1"}]}]}], ")"}]}], "->", 
             RowBox[{"-", "0.5"}]}], ",", " ", 
            RowBox[{"(*", 
             RowBox[{"Sub", " ", "diag"}], "*)"}], "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{
              RowBox[{"{", 
               RowBox[{"i_", ",", "j_"}], "}"}], "/;", 
              RowBox[{"(", 
               RowBox[{
                RowBox[{
                 RowBox[{"j", "-", "i"}], "==", "1"}], "&&", 
                RowBox[{"i", ">", "1"}]}], ")"}]}], "->", "0.5"}]}], " ", 
           RowBox[{"(*", " ", 
            RowBox[{"Super", " ", "diag"}], " ", "*)"}], 
           "\[IndentingNewLine]", "}"}], "]"}]}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
          RowBox[{"Steps", " ", "6"}], ",", " ", "7", ",", " ", "8", ",", " ", 
          RowBox[{"9", ":", " ", 
           RowBox[{
           "Initialize", " ", "right", " ", "side", " ", "vector", " ", 
            StyleBox["f",
             FontWeight->"Bold"]}]}]}], " ", "*)"}], "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"fVec", "[", "n_", "]"}], " ", ":=", " ", 
         RowBox[{"Append", "[", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"Prepend", "[", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"Table", "[", "\[IndentingNewLine]", 
              RowBox[{
               RowBox[{
                RowBox[{"NIntegrate", "[", 
                 RowBox[{
                  RowBox[{
                   RowBox[{"f", "[", "x", "]"}], "*", 
                   FractionBox[
                    RowBox[{"x", "-", 
                    RowBox[{"(", 
                    RowBox[{"a", "+", 
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{"i", "-", "1"}], ")"}], "*", 
                    RowBox[{"h", "[", "n", "]"}]}]}], ")"}]}], 
                    RowBox[{"h", "[", "n", "]"}]]}], ",", " ", 
                  RowBox[{"{", 
                   RowBox[{"x", ",", " ", 
                    RowBox[{"a", "+", 
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{"i", "-", "1"}], ")"}], "*", 
                    RowBox[{"h", "[", "n", "]"}]}]}], ",", " ", 
                    RowBox[{"a", "+", 
                    RowBox[{"i", "*", 
                    RowBox[{"h", "[", "n", "]"}]}]}]}], "}"}]}], "]"}], "+", 
                "\[IndentingNewLine]", 
                RowBox[{"NIntegrate", "[", 
                 RowBox[{
                  RowBox[{
                   RowBox[{"f", "[", "x", "]"}], "*", 
                   FractionBox[
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{"a", "+", 
