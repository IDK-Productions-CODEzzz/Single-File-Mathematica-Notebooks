               RowBox[{"h", "[", "n", "]"}], "2."]]}], ",", " ", 
            RowBox[{"(*", " ", 
             RowBox[{"Main", " ", "diag"}], " ", "*)"}], 
            "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{
              RowBox[{"{", 
               RowBox[{"i_", ",", "j_"}], "}"}], "/;", 
              RowBox[{"(", 
               RowBox[{
                RowBox[{
                 RowBox[{"i", "-", "j"}], "==", "1"}], "&&", 
                RowBox[{"i", "<", 
                 RowBox[{"n", "+", "1"}]}]}], ")"}]}], "->", 
             RowBox[{
              RowBox[{"-", 
               FractionBox["\[Epsilon]", 
