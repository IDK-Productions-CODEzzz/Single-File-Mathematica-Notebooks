Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "Clear", " ", "all", " ", "variables", " ", "to", " ", "ensure", " ", 
    "code", " ", "below", " ", "works"}], " ", "*)"}], "\[IndentingNewLine]", 
  
  RowBox[{
   RowBox[{
    RowBox[{"ClearAll", "[", "\"\<Global`*\>\"", "]"}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Code", " ", "that", " ", "constructs", " ", "the", " ", "visual", " ", 
     "of", " ", "the", " ", "scheme", " ", "approximating", " ", "the", " ", 
     "true", " ", "solution"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"scheme", " ", ":=", " ", 
     RowBox[{"DynamicModule", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "h", ",", " ", "n", ",", " ", "xGrid", ",", " ", "i", ",", " ", 
         "fVec", ",", " ", "matA", ",", " ", "\[Epsilon]", ",", " ", "j", ",",
          " ", "approx", ",", " ", "approxToPlot", ",", " ", "x", ",", " ", 
         "sol", ",", " ", "u", ",", " ", "approxPlot", ",", " ", "solPlot", 
         ",", " ", "plotsTogether"}], "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{
        "Obtain", " ", "true", " ", "solution", " ", "for", " ", "plotting"}],
         " ", "*)"}], "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"sol", "[", 
          RowBox[{"x_", ",", " ", "\[Epsilon]_"}], "]"}], " ", "=", " ", 
         RowBox[{"DSolveValue", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"-", "\[Epsilon]"}], "*", 
               RowBox[{
                RowBox[{"u", "''"}], "[", "x", "]"}]}], "+", 
              RowBox[{
               RowBox[{"u", "'"}], "[", "x", "]"}]}], "==", "1"}], "&&", 
            RowBox[{
             RowBox[{"u", "[", "0", "]"}], "==", "0"}], "&&", 
            RowBox[{
             RowBox[{"u", "[", "1", "]"}], "==", "0"}]}], ",", " ", 
           RowBox[{"u", "[", "x", "]"}], ",", " ", "x"}], "]"}]}], ";", 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{"Define", " ", "h"}], " ", "*)"}], "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"h", "[", "n_", "]"}], " ", ":=", " ", 
         FractionBox["1.", "n"]}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{"Construct", " ", "the", " ", "grid"}], " ", "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{
