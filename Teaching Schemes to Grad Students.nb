         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"i", "*", 
            RowBox[{"h", "[", "n", "]"}]}], ",", " ", 
           RowBox[{"{", 
            RowBox[{"i", ",", " ", "0", ",", " ", "n"}], "}"}]}], "]"}]}], 
        ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
