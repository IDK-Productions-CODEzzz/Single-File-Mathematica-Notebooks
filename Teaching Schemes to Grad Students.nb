Cell[TextData[{
 "To begin, define our subinterval width ",
 Cell[BoxData[
  FormBox[
   RowBox[{"h", "=", 
    FractionBox[
     RowBox[{"b", "-", "a"}], "n"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "d6496626-69d7-4616-9095-ee56146b86ba"],
