   RowBox[{
    RowBox[{"u0", " ", "=", " ", "0"}], ";"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Code", " ", "that", " ", "constructs", " ", "the", " ", "visual", " ", 
     "of", " ", "the", " ", "scheme", " ", "approximating", " ", "the", " ", 
     "true", " ", "solution"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"scheme", "[", 
      RowBox[{"inF_", ",", " ", 
       RowBox[{"{", 
        RowBox[{"inA_", ",", " ", "inB_"}], "}"}], ",", " ", "inU0_"}], "]"}],
      " ", ":=", " ", 
     RowBox[{"DynamicModule", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "trueSol", ",", " ", "u", ",", " ", "x", ",", " ", "fastF", ",", " ", 
         "h", ",", " ", "n", ",", " ", "j", ",", " ", "i", ",", " ", 
         "approxSol", ",", " ", "err", ",", " ", "\[Alpha]", ",", " ", 
         "plotLabel"}], "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{"Error", " ", "check", " ", "domain"}], " ", "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"inA", ">=", "inB"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"Print", "[", 
            RowBox[{"Style", "[", 
             RowBox[{
             "\"\<ERROR: Invalid domain! Requires \
\!\(\*Cell[\"a<b\",ExpressionUUID->\"46bb2b5b-acc9-425a-a365-5076e2b748e9\"]\)\
...\>\"", ",", " ", "Red"}], "]"}], "]"}], ";", 
           RowBox[{"Return", "[", 
            RowBox[{"Null", ",", " ", "Module"}], "]"}]}], ",", 
          "\[IndentingNewLine]", "Null"}], "]"}], ";", "\[IndentingNewLine]", 
        
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"Or", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"Head", "[", "inA", "]"}], "==", "Complex"}], ",", " ", 
            RowBox[{
             RowBox[{"Head", "[", "inB", "]"}], "==", "Complex"}]}], "]"}], 
          ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"Print", "[", 
            RowBox[{"Style", "[", 
             RowBox[{
             "\"\<ERROR: Invalid domain! Requires \
\!\(\*Cell[\"a\",ExpressionUUID->\"f5168564-d1ec-42bd-93ff-7ebdfbe2a6b9\"]\) \
and \!\(\*Cell[\"b\",ExpressionUUID->\"308a092e-c3d2-4979-b4f6-7f884efbeacd\"]\
\) to be real...\>\"", ",", " ", "Red"}], "]"}], "]"}], ";", 
           RowBox[{"Return", "[", 
            RowBox[{"Null", ",", " ", "Module"}], "]"}]}], ",", 
          "\[IndentingNewLine]", "Null"}], "]"}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{"Obtain", " ", "true", " ", "solution"}], " ", "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"trueSol", " ", "=", " ", 
