Cell[BoxData[
 RowBox[{
  RowBox[{
   UnderscriptBox["max", "i"], 
   RowBox[{"(", 
    RowBox[{"|", 
     RowBox[{
      RowBox[{
       RowBox[{"(", "\[ScriptCapitalL]u", ")"}], 
       RowBox[{"(", 
        SubscriptBox["x", "i"], ")"}]}], "-", 
      SubscriptBox[
       RowBox[{"(", "Lu", ")"}], "i"]}], "|"}], ")"}]}], "=", 
