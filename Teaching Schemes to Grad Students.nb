Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "Clear", " ", "all", " ", "variables", " ", "to", " ", "ensure", " ", 
    "code", " ", "below", " ", "works"}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"ClearAll", "[", "\"\<Global`*\>\"", "]"}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"Input", " ", "your", " ", "custom", " ", "variables"}], " ", 
    "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"f", "[", "x_", "]"}], " ", "=", " ", 
     RowBox[{"Cos", "[", "x", "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"{", 
      RowBox[{"a", ",", " ", "b"}], "}"}], " ", "=", " ", 
     RowBox[{"{", 
      RowBox[{"0", ",", " ", 
       RowBox[{"2", "*", "\[Pi]"}]}], "}"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"u0", " ", "=", " ", "0"}], ";"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Code", " ", "that", " ", "constructs", " ", "the", " ", "visual", " ", 
     "of", " ", "the", " ", "scheme", " ", "approximating", " ", "the", " ", 
     "true", " ", "solution"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"scheme", "[", 
      RowBox[{"inF_", ",", " ", 
       RowBox[{"{", 
        RowBox[{"inA_", ",", " ", "inB_"}], "}"}], ",", " ", "inU0_"}], "]"}],
      " ", ":=", " ", 
     RowBox[{"DynamicModule", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "trueSol", ",", " ", "u", ",", " ", "x", ",", " ", "fastF", ",", " ", 
         "h", ",", " ", "n", ",", " ", "j", ",", " ", "i", ",", " ", 
         "approxSol", ",", " ", "err"}], "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{"Error", " ", "check", " ", "domain"}], " ", "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"inA", ">=", "inB"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"Print", "[", 
            RowBox[{"Style", "[", 
             RowBox[{
             "\"\<ERROR: Invalid domain! Requires \
\!\(\*Cell[\"a<b\",ExpressionUUID->\"06b342be-00ba-442c-b7ac-161124666b85\"]\)\
...\>\"", ",", " ", "Red"}], "]"}], "]"}], ";", 
           RowBox[{"Return", "[", 
            RowBox[{"Null", ",", " ", "Module"}], "]"}]}], ",", 
          "\[IndentingNewLine]", "Null"}], "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"Or", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"Head", "[", "inA", "]"}], "==", "Complex"}], ",", " ", 
            RowBox[{
             RowBox[{"Head", "[", "inB", "]"}], "==", "Complex"}]}], "]"}], 
          ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"Print", "[", 
            RowBox[{"Style", "[", 
             RowBox[{
             "\"\<ERROR: Invalid domain! Requires \
\!\(\*Cell[\"a\",ExpressionUUID->\"3f24f73d-79d0-41b6-a3b0-2954130bcdea\"]\) \
and \!\(\*Cell[\"b\",ExpressionUUID->\"37351b32-e236-4759-8535-4ba7406dcd82\"]\
\) to be real...\>\"", ",", " ", "Red"}], "]"}], "]"}], ";", 
           RowBox[{"Return", "[", 
            RowBox[{"Null", ",", " ", "Module"}], "]"}]}], ",", 
          "\[IndentingNewLine]", "Null"}], "]"}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{"Obtain", " ", "true", " ", "solution"}], " ", "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"trueSol", " ", "=", " ", 
         RowBox[{"NDSolveValue", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"u", "'"}], "[", "x", "]"}], "==", 
              RowBox[{"inF", "[", "x", "]"}]}], ",", " ", 
             RowBox[{
              RowBox[{"u", "[", "inA", "]"}], "==", "inU0"}]}], "}"}], ",", 
           " ", "u", ",", " ", 
           RowBox[{"{", 
            RowBox[{"x", ",", " ", "inA", ",", " ", "inB"}], "}"}]}], "]"}]}],
         ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "Compile", " ", "f", " ", "so", " ", "code", " ", "runs", " ", 
          "faster"}], " ", "*)"}], "\[IndentingNewLine]", 
        RowBox[{"fastF", " ", "=", " ", 
         RowBox[{"Compile", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"{", 
             RowBox[{"x", ",", " ", "_Real"}], "}"}], "}"}], ",", " ", 
           RowBox[{"inF", "[", "x", "]"}], ",", " ", 
           RowBox[{"CompilationTarget", "->", "\"\<C\>\""}], ",", " ", 
           RowBox[{"Parallelization", "->", "True"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "Define", " ", Cell[
          "h",ExpressionUUID->"0011cfc8-efd8-4c21-bde7-4594c64a9100"], " ", 
          "delayed", " ", "for", " ", "manipulating", " ", "across", " ", 
          Cell["n",ExpressionUUID->"37df8a92-54ef-4c9a-b8c8-4c7ecc04a4cf"]}], 
         " ", "*)"}], "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"h", "[", "n_", "]"}], " ", ":=", " ", 
         RowBox[{"N", "[", 
          FractionBox[
           RowBox[{"inB", "-", "inA"}], "n"], "]"}]}], ";", 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "Define", " ", "approximate", " ", "solution", " ", "delayed", " ", 
          "for", " ", "manipulating", " ", "across", " ", Cell[
          "n",ExpressionUUID->"dd09c402-8103-4283-999f-bd917d61eea4"]}], " ", 
         "*)"}], "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"approxSol", "[", "n_", "]"}], " ", ":=", " ", 
         RowBox[{"Thread", "[", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"NestList", "[", 
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{"h", "[", "n", "]"}], "+", "#"}], "&"}], ",", " ", 
              "inA", ",", " ", "n"}], "]"}], ",", " ", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"j", " ", "=", " ", "1"}], ";", 
              RowBox[{"NestList", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{
                  RowBox[{
                   RowBox[{"h", "[", "n", "]"}], "*", 
                   RowBox[{"fastF", "[", 
                    RowBox[{"inA", "+", 
                    RowBox[{
                    RowBox[{"h", "[", "n", "]"}], "*", 
                    RowBox[{"j", "++"}]}]}], "]"}]}], "+", "#"}], "&"}], ",", 
                " ", "inU0", ",", " ", "n"}], "]"}]}], ")"}]}], "}"}], 
          "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "Define", " ", "error", " ", "delayed", " ", "for", " ", 
          "manipulating", " ", "across", " ", "n"}], " ", "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"err", "[", "n_", "]"}], " ", ":=", " ", 
         RowBox[{"Norm", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"approxSol", "[", "n", "]"}], "[", 
             RowBox[{"[", 
              RowBox[{"All", ",", " ", "2"}], "]"}], "]"}], "-", 
            RowBox[{"trueSol", "[", 
             RowBox[{"Table", "[", 
              RowBox[{
               RowBox[{"inA", "+", 
                RowBox[{"i", "*", 
                 RowBox[{"h", "[", "n", "]"}]}]}], ",", " ", 
               RowBox[{"{", 
                RowBox[{"i", ",", " ", "0", ",", " ", "n"}], "}"}]}], "]"}], 
             "]"}]}], ",", " ", "2"}], "]"}]}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "Code", " ", "to", " ", "display", " ", "the", " ", "approximation", 
          " ", "visually"}], " ", "*)"}], "\[IndentingNewLine]", 
        RowBox[{"Manipulate", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"Show", "[", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"Plot", "[", 
             RowBox[{
              RowBox[{"trueSol", "[", "x", "]"}], ",", " ", 
              RowBox[{"{", 
               RowBox[{"x", ",", " ", "inA", ",", " ", "inB"}], "}"}], ",", 
              " ", 
              RowBox[{"PlotLabel", "->", 
               RowBox[{"\"\<Forward Euler Scheme\\nEuclidean Error: \>\"", "<>", 
                RowBox[{"ToString", "[", 
                 RowBox[{"err", "[", "n", "]"}], "]"}]}]}]}], "]"}], ",", 
            "\[IndentingNewLine]", 
            RowBox[{"ListPlot", "[", 
             RowBox[{
              RowBox[{"approxSol", "[", "n", "]"}], ",", " ", 
              RowBox[{"PlotStyle", "->", "Red"}]}], "]"}]}], 
           "\[IndentingNewLine]", "]"}], ",", "\[IndentingNewLine]", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"n", ",", " ", "20", ",", " ", "\"\<Nodes\>\""}], "}"}], 
            ",", " ", "1", ",", " ", "2000", ",", " ", "1"}], "}"}]}], 
         "]"}]}]}], "\[IndentingNewLine]", "]"}]}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"Call", " ", "code", " ", "to", " ", "run"}], " ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{"scheme", "[", 
    RowBox[{"f", ",", " ", 
     RowBox[{"{", 
      RowBox[{"a", ",", " ", "b"}], "}"}], ",", " ", "u0"}], 
    "]"}]}]}]], "Input",
