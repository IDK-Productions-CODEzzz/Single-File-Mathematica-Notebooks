Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["\[Phi]", 
       RowBox[{"i", ",", "j"}]], 
      RowBox[{"(", "z", ")"}]}], "=", 
     RowBox[{"{", GridBox[{
        {
         RowBox[{
          RowBox[{
           FractionBox[
            RowBox[{"z", "-", 
             SubscriptBox["x", "j"]}], 
            RowBox[{
             SubscriptBox["x", "i"], "-", 
             SubscriptBox["x", "j"]}]], " ", "if", " ", "i"}], "\[NotEqual]", 
          "j"}]},
        {
         RowBox[{
          RowBox[{"1", " ", "if", " ", "i"}], "=", "j"}]}
       }], "}"}]}], ",", " ", 
    RowBox[{
     RowBox[{
      SubscriptBox["p", "n"], 
      RowBox[{"(", "z", ")"}]}], "=", 
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"i", "=", "1"}], "n"], 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["y", "i"], "*", 
        RowBox[{
         UnderoverscriptBox["\[Product]", 
          RowBox[{"j", "=", "1"}], "n"], 
         RowBox[{"(", 
          RowBox[{
           SubscriptBox["\[Phi]", 
            RowBox[{"i", ",", "j"}]], 
           RowBox[{"(", "z", ")"}]}], ")"}]}]}], ")"}]}]}]}], " ", "*)"}], 
  "\n", 
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{
     SubscriptBox["p", "n"], 
     RowBox[{"(", 
      SubscriptBox["x", "i"], ")"}]}], "=", 
    RowBox[{"f", 
     RowBox[{"(", 
      SubscriptBox["x", "i"], ")"}]}]}], " ", "*)"}], "\n", "\n", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"p", "[", 
      RowBox[{"z_", ",", "xVec_", ",", "yVec_"}], "]"}], " ", ":=", "\n", 
     RowBox[{"Sum", "[", "\n", "\t", 
      RowBox[{
       RowBox[{
        RowBox[{"yVec", "[", 
         RowBox[{"[", "i", "]"}], "]"}], " ", "*", " ", 
        RowBox[{"Product", "[", "\n", "\t\t", 
         RowBox[{
          RowBox[{"Piecewise", "[", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               FractionBox[
                RowBox[{"z", "-", 
                 RowBox[{"xVec", "[", 
                  RowBox[{"[", "j", "]"}], "]"}]}], 
                RowBox[{
                 RowBox[{"xVec", "[", 
                  RowBox[{"[", "i", "]"}], "]"}], "-", 
                 RowBox[{"xVec", "[", 
                  RowBox[{"[", "j", "]"}], "]"}]}]], ",", " ", 
               RowBox[{"Not", "[", 
                RowBox[{"i", "==", "j"}], "]"}]}], "}"}], ",", " ", 
             RowBox[{"{", 
              RowBox[{"1", ",", " ", 
               RowBox[{"i", "==", "j"}]}], "}"}]}], "}"}], "]"}], ",", "\n", 
          "\t", 
          RowBox[{"{", 
           RowBox[{"j", ",", " ", "1", ",", " ", 
            RowBox[{"Min", "[", 
             RowBox[{
              RowBox[{"Length", "[", "xVec", "]"}], ",", " ", 
              RowBox[{"Length", "[", "yVec", "]"}]}], "]"}]}], "}"}]}], 
         "]"}]}], ",", "\n", 
       RowBox[{"{", 
        RowBox[{"i", ",", " ", "1", ",", " ", 
         RowBox[{"Min", "[", 
          RowBox[{
           RowBox[{"Length", "[", "xVec", "]"}], ",", " ", 
           RowBox[{"Length", "[", "yVec", "]"}]}], "]"}]}], "}"}]}], "]"}]}], 
    ";"}], "\n", "\n", 
   RowBox[{
   "Print", "[", "\"\<The interpolating polynomial has been stored.\>\"", 
    "]"}]}]}]], "Code",
 CellChangeTimes->{{3.8764862478874025`*^9, 3.8764865533441744`*^9}, 
   3.876486583787121*^9, {3.876487904482363*^9, 3.876487932386339*^9}, {
